package edu.nashcc.cars;

public class Ford extends Auto {

	
	public Ford() {
		super("Ford");
		setPrice();
	}
	public void setPrice(){
	this.price = 20000; // $20,000	
	}
	public String modelOptions(){
		StringBuilder sb = new StringBuilder();
		sb.append("1 - Fiesta\n");
		sb.append("2 - Explorer\n");
		
		return sb.toString();
	}

}
