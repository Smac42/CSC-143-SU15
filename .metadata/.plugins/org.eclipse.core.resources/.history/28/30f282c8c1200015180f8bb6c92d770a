package edu.nashcc.cars;

public class Chevy extends Auto {

	public Chevy(String make) {
		super(make);
		setPrice();
	}
	public void setPrice(){
		this.price = 22000; // $22,000
	}
}
class Camaro extends Chevy {

	private String model;
	public Camaro(String make) {
		super(make);
		setModel();
	}
	public void setModel(){
		this.model = "Camaro";
	}
	public String getModel(){
		return this.model;
	}
}