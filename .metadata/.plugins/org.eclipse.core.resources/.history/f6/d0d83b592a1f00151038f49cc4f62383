package edu.nashcc.debug;


public class DebugOceanLiner extends DebugBoat
{
   public DebugOceanLiner()
   {
      DebugBoat("ocean liner ");
      setPassengers();
      setPower();
   }
   public void setPassengers()
   {
      super.passengers = 2400;
   }
   public void power()
   {
      super.power = "four engines";
   }
}
